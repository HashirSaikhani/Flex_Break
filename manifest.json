{
  "manifest_version": 3,
  "name": "Flex App Login Logger",
  "version": "1.0",
  "permissions": ["scripting"],
  "host_permissions": [
    "https://flexstudent.nu.edu.pk/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://flexstudent.nu.edu.pk/Login*"],
      "js": ["content.js"]
    }
  ]
}
